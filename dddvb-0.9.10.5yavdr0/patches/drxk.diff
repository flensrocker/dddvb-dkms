-	/* Program frequency shifter with tuner offset compensation */
-	/* frequencyShift += tunerFreqOffset; TODO */
-	status = Write32(state, IQM_FS_RATE_OFS_LO__A , 
-			 state->m_IqmFsRateOfs, 0);
-	return status;
-}
